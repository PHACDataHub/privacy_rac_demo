      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-personal_opinion_on_person(A,B,C),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-personal_opinion_on_person(A,B,C),datetime(End))
