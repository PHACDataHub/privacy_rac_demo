    scasp_encoding: "blawx_attribute(information,is_address_of,individual).\nblawx_attribute_nlg(is_address_of,ov,\"\",\"is
      the address of\",\"\").\n:- dynamic is_address_of/2.\n#pred is_address_of(X,Y)
      :: '@(X) is the address of @(Y)'.\n#pred holds(user,is_address_of,X,Y) :: 'it
      is provided as a fact that @(X) is the address of @(Y)'.\n#pred holds(user,-is_address_of,X,Y)
      :: 'it is provided as a fact that it is not the case that @(X) is the address
      of @(Y)'.\n#pred holds(Z,is_address_of,X,Y) :: 'the conclusion in @(Z) that
      @(X) is the address of @(Y) holds'.\n#pred holds(Z,-is_address_of,X,Y) :: 'the
      conclusion in @(Z) that it is not the case that @(X) is the address of @(Y)
      holds'.\n#pred according_to(Z,is_address_of,X,Y) :: 'according to @(Z), @(X)
      is the address of @(Y)'.\n#pred according_to(Z,-is_address_of,X,Y) :: 'according
      to @(Z), it is not the case that @(X) is the address of @(Y)'.\n#pred blawx_defeated(Z,is_address_of,X,Y)
      :: 'the conclusion in @(Z) that @(X) is the address of @(Y) is defeated'.\n#pred
      blawx_defeated(Z,-is_address_of,X,Y) :: 'the conclusion in @(Z) that @(X) is
      the address of @(Y) is defeated'.\n#pred blawx_initially(is_address_of(X,Y))
      :: 'that @(X) is the address of @(Y) holds initially'.\n#pred blawx_initially(-is_address_of(X,Y))
      :: 'that it is not the case that @(X) is the address of @(Y) holds initially'.\n#pred
      blawx_ultimately(is_address_of(X,Y)) :: 'that @(X) is the address of @(Y) holds
      ultimately'.\n#pred blawx_ultimately(-is_address_of(X,Y)) :: 'that it is not
      the case that @(X) is the address of @(Y) holds ultimately'.\n#pred blawx_as_of(is_address_of(X,Y),T)
      :: 'that @(X) is the address of @(Y) holds at @(T)'.\n#pred blawx_as_of(-is_address_of(X,Y),T)
      :: 'that it is not the case that @(X) is the address of @(Y) holds at @(T)'.\n#pred
      blawx_during(T1,is_address_of(X,Y),T2) :: 'that @(X) is the address of @(Y)
      held between @(T1) and @(T2)'.\n#pred blawx_during(T1,-is_address_of(X,Y),T2)
      :: 'that it is not the case that @(X) is the address of @(Y) held between @(T1)
      and @(T2)'.\n#pred blawx_becomes(is_address_of(X,Y),T) :: 'that @(X) is the
      address of @(Y) became true at @(T)'.\n#pred blawx_becomes(-is_address_of(X,Y),T)
      :: 'that it is not the case that @(X) is the address of @(Y) became true at
      @(T)'.\n#pred blawx_not_interrupted(Start,is_address_of(X,Y),End) :: '@(X) is
      the address of @(Y) remained the case between @(Start) and @(End)'.\n#pred blawx_not_interrupted(Start,-is_address_of(X,Y),End)
      :: 'it is not the case that @(X) is the address of @(Y) remained the case between
      @(Start) and @(End)'.\nblawx_not_interrupted(datetime(Start),is_address_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-is_address_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-is_address_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(is_address_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),is_address_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-is_address_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-is_address_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(is_address_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),is_address_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-is_address_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-is_address_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(is_address_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),is_address_of(X,Y),datetime(eot))
      :- blawx_initially(is_address_of(X,Y)), blawx_ultimately(is_address_of(X,Y)),
      findall(Time,blawx_becomes(-is_address_of(X,Y),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-is_address_of(X,Y),datetime(eot))
      :- blawx_initially(-is_address_of(X,Y)), blawx_ultimately(-is_address_of(X,Y)),
      findall(Time,blawx_becomes(is_address_of(X,Y),datetime(Time)),[]).\nblawx_as_of(is_address_of(X,Y),datetime(Time))
      :- blawx_initially(is_address_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),is_address_of(X,Y),datetime(BeforeT)).\nblawx_as_of(is_address_of(X,Y),datetime(Time))
      :- blawx_becomes(is_address_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),is_address_of(X,Y),datetime(Time)).\nblawx_as_of(-is_address_of(X,Y),datetime(Time))
