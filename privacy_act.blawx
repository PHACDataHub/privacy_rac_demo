      blawx_not_interrupted(Start,-under_control_of(X,Y),End) :: 'it is not the case
      that @(X) is under the control of @(Y) remained the case between @(Start) and
      @(End)'.\nblawx_not_interrupted(datetime(Start),under_control_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-under_control_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-under_control_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(under_control_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),under_control_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-under_control_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-under_control_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(under_control_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),under_control_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-under_control_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-under_control_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(under_control_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),under_control_of(X,Y),datetime(eot))
      :- blawx_initially(under_control_of(X,Y)), blawx_ultimately(under_control_of(X,Y)),
      findall(Time,blawx_becomes(-under_control_of(X,Y),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-under_control_of(X,Y),datetime(eot))
