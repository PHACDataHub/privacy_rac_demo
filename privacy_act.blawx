      blawx_not_interrupted(datetime(Start),-information(X),datetime(eot)).\nblawx_during(datetime(bot),-information(X),datetime(eot))
      :- blawx_initially(-information(X)), blawx_ultimately(-information(X)), blawx_becomes(-information(X),datetime(Start)),
      blawx_not_interrupted(datetime(bot),-information(X),datetime(eot)).\nblawx_attribute(information,identifiably_about,individual).\nblawx_attribute_nlg(identifiably_about,ov,\"\",\"is
      identifiably about\",\"\").\n:- dynamic identifiably_about/2.\n#pred identifiably_about(X,Y)
