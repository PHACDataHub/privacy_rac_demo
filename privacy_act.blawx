    scasp_encoding: "blawx_category(entity).\nblawx_category_nlg(entity,\"\",\"is
      an entity\").\n:- dynamic entity/1.\n#pred entity(X) :: '@(X) is an entity'.\n#pred
      holds(user,entity,X) :: 'it is provided as a fact that @(X) is an entity'.\n#pred
      holds(user,-entity,X) :: 'it is provided as a fact that it is not the case that
      @(X) is an entity'.\n#pred holds(Z,entity,X) :: 'the conclusion in @(Z) that
      @(X) is an entity holds'.\n#pred holds(Z,-entity,X) :: 'the conclusion in @(Z)
      that it is not the case that @(X) is an entity holds'.\n#pred according_to(Z,entity,X)
      :: 'according to @(Z), @(X) is an entity'.\n#pred according_to(Z,-entity,X)
      :: 'according to @(Z), it is not the case that @(X) is an entity'.\n#pred blawx_defeated(Z,entity,X)
      :: 'the conclusion in @(Z) that @(X) is an entity is defeated'.\n#pred blawx_defeated(Z,-entity,X)
      :: 'the conclusion in @(Z) that it is not the case that @(X) is an entity is
      defeated'.\n#pred blawx_initially(entity(X)) :: 'that @(X) is an entity holds
      initially'.\n#pred blawx_initially(-entity(X)) :: 'that it is not the case that
      @(X) is an entity holds initially'.\n#pred blawx_ultimately(entity(X)) :: 'that
      @(X) is an entity holds ultimately'.\n#pred blawx_ultimately(-entity(X)) ::
      'that it is not the case that @(X) is an entity holds ultimately'.\n#pred blawx_as_of(entity(X),T)
      :: 'that @(X) is an entity holds at @(T)'.\n#pred blawx_as_of(-entity(X),T)
      :: 'that it is not the case that @(X) is an entity holds at @(T)'.\n#pred blawx_during(T1,entity(X),T2)
      :: 'that @(X) is an entity held between @(T1) and @(T2)'.\n#pred blawx_during(T1,-entity(X),T2)
      :: 'that it is not the case that @(X) is an entity held between @(T1) and @(T2)'.\n#pred
      blawx_becomes(entity(X),T) :: 'that @(X) is an entity became true at @(T)'.\n#pred
      blawx_becomes(-entity(X),T) :: 'that it is not the case that @(X) is an entity
      became true at @(T)'.\n#pred blawx_not_interrupted(Start,entity(X),End) :: '@(X)
      is an entity remained the case between @(Start) and @(End)'.\n#pred blawx_not_interrupted(Start,-entity(X),End)
      :: 'it is not the case that @(X) is an entity remained the case between @(Start)
      and @(End)'.\nblawx_not_interrupted(datetime(Start),entity(X),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-entity(X),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-entity(X),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(entity(X),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),entity(X),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-entity(X),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-entity(X),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(entity(X),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),entity(X),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-entity(X),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-entity(X),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(entity(X),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),entity(X),datetime(eot))
      :- blawx_initially(entity(X)), blawx_ultimately(entity(X)), findall(Time,blawx_becomes(-entity(X),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-entity(X),datetime(eot))
      :- blawx_initially(-entity(X)), blawx_ultimately(-entity(X)), findall(Time,blawx_becomes(entity(X),datetime(Time)),[]).\nblawx_as_of(entity(X),datetime(Time))
      :- blawx_initially(entity(X)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),entity(X),datetime(BeforeT)).\nblawx_as_of(entity(X),datetime(Time))
      :- blawx_becomes(entity(X),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),entity(X),datetime(Time)).\nblawx_as_of(-entity(X),datetime(Time))
      :- blawx_initially(-entity(X)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),-entity(X),datetime(BeforeT)).\nblawx_as_of(-entity(X),datetime(Time))
      :- blawx_becomes(-entity(X),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),-entity(X),datetime(Time)).\nblawx_during(datetime(Start),entity(X),datetime(End))
      :- blawx_becomes(entity(X),datetime(Start)), blawx_becomes(-entity(X),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),entity(X),datetime(End)).\nblawx_during(datetime(bot),entity(X),datetime(End))
      :- blawx_initially(entity(X)), blawx_becomes(-entity(X),datetime(End)), blawx_not_interrupted(datetime(bot),entity(X),datetime(End)).\nblawx_during(datetime(Start),entity(X),datetime(eot))
      :- blawx_ultimately(entity(X)), blawx_becomes(-entity(X),datetime(Start)), blawx_not_interrupted(datetime(Start),entity(X),datetime(eot)).\nblawx_during(datetime(bot),entity(X),datetime(eot))
      :- blawx_initially(entity(X)), blawx_ultimately(entity(X)), blawx_becomes(-entity(X),datetime(Start)),
      blawx_not_interrupted(datetime(bot),entity(X),datetime(eot)).\nblawx_during(datetime(Start),-entity(X),datetime(End))
      :- blawx_becomes(-entity(X),datetime(Start)), blawx_becomes(entity(X),datetime(End)),
