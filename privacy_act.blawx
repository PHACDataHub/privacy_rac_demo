      :- blawx_initially(individual(X)), blawx_becomes(-individual(X),datetime(End)),
      blawx_not_interrupted(datetime(bot),individual(X),datetime(End)).\nblawx_during(datetime(Start),individual(X),datetime(eot))
      :- blawx_ultimately(individual(X)), blawx_becomes(-individual(X),datetime(Start)),
      blawx_not_interrupted(datetime(Start),individual(X),datetime(eot)).\nblawx_during(datetime(bot),individual(X),datetime(eot))
      :- blawx_initially(individual(X)), blawx_ultimately(individual(X)), blawx_becomes(-individual(X),datetime(Start)),
      blawx_not_interrupted(datetime(bot),individual(X),datetime(eot)).\nblawx_during(datetime(Start),-individual(X),datetime(End))
      :- blawx_becomes(-individual(X),datetime(Start)), blawx_becomes(individual(X),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),-individual(X),datetime(End)).\nblawx_during(datetime(bot),-individual(X),datetime(End))
      :- blawx_initially(-individual(X)), blawx_becomes(individual(X),datetime(End)),
      blawx_not_interrupted(datetime(bot),-individual(X),datetime(End)).\nblawx_during(datetime(Start),-individual(X),datetime(eot))
      :- blawx_ultimately(-individual(X)), blawx_becomes(individual(X),datetime(Start)),
      blawx_not_interrupted(datetime(Start),-individual(X),datetime(eot)).\nblawx_during(datetime(bot),-individual(X),datetime(eot))
      :- blawx_initially(-individual(X)), blawx_ultimately(-individual(X)), blawx_becomes(-individual(X),datetime(Start)),
      blawx_not_interrupted(datetime(bot),-individual(X),datetime(eot)).\nblawx_category(information).\nblawx_category_nlg(information,\"\",\"is
      information\").\n:- dynamic information/1.\n#pred information(X) :: '@(X) is
