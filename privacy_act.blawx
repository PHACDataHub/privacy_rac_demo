    scasp_encoding: "blawx_category(individual).\nblawx_category_nlg(individual,\"\",\"is
      an individual\").\n:- dynamic individual/1.\n#pred individual(X) :: '@(X) is
      an individual'.\n#pred holds(user,individual,X) :: 'it is provided as a fact
      that @(X) is an individual'.\n#pred holds(user,-individual,X) :: 'it is provided
      as a fact that it is not the case that @(X) is an individual'.\n#pred holds(Z,individual,X)
      :: 'the conclusion in @(Z) that @(X) is an individual holds'.\n#pred holds(Z,-individual,X)
      :: 'the conclusion in @(Z) that it is not the case that @(X) is an individual
      holds'.\n#pred according_to(Z,individual,X) :: 'according to @(Z), @(X) is an
      individual'.\n#pred according_to(Z,-individual,X) :: 'according to @(Z), it
      is not the case that @(X) is an individual'.\n#pred blawx_defeated(Z,individual,X)
      :: 'the conclusion in @(Z) that @(X) is an individual is defeated'.\n#pred blawx_defeated(Z,-individual,X)
      :: 'the conclusion in @(Z) that it is not the case that @(X) is an individual
      is defeated'.\n#pred blawx_initially(individual(X)) :: 'that @(X) is an individual
      holds initially'.\n#pred blawx_initially(-individual(X)) :: 'that it is not
      the case that @(X) is an individual holds initially'.\n#pred blawx_ultimately(individual(X))
      :: 'that @(X) is an individual holds ultimately'.\n#pred blawx_ultimately(-individual(X))
      :: 'that it is not the case that @(X) is an individual holds ultimately'.\n#pred
      blawx_as_of(individual(X),T) :: 'that @(X) is an individual holds at @(T)'.\n#pred
      blawx_as_of(-individual(X),T) :: 'that it is not the case that @(X) is an individual
      holds at @(T)'.\n#pred blawx_during(T1,individual(X),T2) :: 'that @(X) is an
      individual held between @(T1) and @(T2)'.\n#pred blawx_during(T1,-individual(X),T2)
      :: 'that it is not the case that @(X) is an individual held between @(T1) and
      @(T2)'.\n#pred blawx_becomes(individual(X),T) :: 'that @(X) is an individual
      became true at @(T)'.\n#pred blawx_becomes(-individual(X),T) :: 'that it is
      not the case that @(X) is an individual became true at @(T)'.\n#pred blawx_not_interrupted(Start,individual(X),End)
      :: '@(X) is an individual remained the case between @(Start) and @(End)'.\n#pred
      blawx_not_interrupted(Start,-individual(X),End) :: 'it is not the case that
      @(X) is an individual remained the case between @(Start) and @(End)'.\nblawx_not_interrupted(datetime(Start),individual(X),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-individual(X),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-individual(X),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(individual(X),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),individual(X),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-individual(X),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-individual(X),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(individual(X),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),individual(X),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-individual(X),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-individual(X),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(individual(X),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),individual(X),datetime(eot))
      :- blawx_initially(individual(X)), blawx_ultimately(individual(X)), findall(Time,blawx_becomes(-individual(X),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-individual(X),datetime(eot))
      :- blawx_initially(-individual(X)), blawx_ultimately(-individual(X)), findall(Time,blawx_becomes(individual(X),datetime(Time)),[]).\nblawx_as_of(individual(X),datetime(Time))
      :- blawx_initially(individual(X)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),individual(X),datetime(BeforeT)).\nblawx_as_of(individual(X),datetime(Time))
      :- blawx_becomes(individual(X),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),individual(X),datetime(Time)).\nblawx_as_of(-individual(X),datetime(Time))
      :- blawx_initially(-individual(X)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),-individual(X),datetime(BeforeT)).\nblawx_as_of(-individual(X),datetime(Time))
      :- blawx_becomes(-individual(X),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),-individual(X),datetime(Time)).\nblawx_during(datetime(Start),individual(X),datetime(End))
      :- blawx_becomes(individual(X),datetime(Start)), blawx_becomes(-individual(X),datetime(End)),
