      findall(Time,blawx_becomes(under_control_of(X,Y),datetime(Time)),[]).\nblawx_as_of(under_control_of(X,Y),datetime(Time))
      :- blawx_initially(under_control_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),under_control_of(X,Y),datetime(BeforeT)).\nblawx_as_of(under_control_of(X,Y),datetime(Time))
      :- blawx_becomes(under_control_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),under_control_of(X,Y),datetime(Time)).\nblawx_as_of(-under_control_of(X,Y),datetime(Time))
      :- blawx_initially(-under_control_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),-under_control_of(X,Y),datetime(BeforeT)).\nblawx_as_of(-under_control_of(X,Y),datetime(Time))
      :- blawx_becomes(-under_control_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),-under_control_of(X,Y),datetime(Time)).\nblawx_during(datetime(Start),under_control_of(X,Y),datetime(End))
      :- blawx_becomes(under_control_of(X,Y),datetime(Start)), blawx_becomes(-under_control_of(X,Y),datetime(End)),
