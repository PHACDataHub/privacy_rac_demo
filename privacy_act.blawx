    scasp_encoding: "blawx_attribute(information,is_opinion_of,individual).\nblawx_attribute_nlg(is_opinion_of,ov,\"\",\"is
      a personal opinion or view of\",\"\").\n:- dynamic is_opinion_of/2.\n#pred is_opinion_of(X,Y)
      :: '@(X) is a personal opinion or view of @(Y)'.\n#pred holds(user,is_opinion_of,X,Y)
      :: 'it is provided as a fact that @(X) is a personal opinion or view of @(Y)'.\n#pred
      holds(user,-is_opinion_of,X,Y) :: 'it is provided as a fact that it is not the
      case that @(X) is a personal opinion or view of @(Y)'.\n#pred holds(Z,is_opinion_of,X,Y)
      :: 'the conclusion in @(Z) that @(X) is a personal opinion or view of @(Y) holds'.\n#pred
      holds(Z,-is_opinion_of,X,Y) :: 'the conclusion in @(Z) that it is not the case
      that @(X) is a personal opinion or view of @(Y) holds'.\n#pred according_to(Z,is_opinion_of,X,Y)
      :: 'according to @(Z), @(X) is a personal opinion or view of @(Y)'.\n#pred according_to(Z,-is_opinion_of,X,Y)
      :: 'according to @(Z), it is not the case that @(X) is a personal opinion or
      view of @(Y)'.\n#pred blawx_defeated(Z,is_opinion_of,X,Y) :: 'the conclusion
      in @(Z) that @(X) is a personal opinion or view of @(Y) is defeated'.\n#pred
      blawx_defeated(Z,-is_opinion_of,X,Y) :: 'the conclusion in @(Z) that @(X) is
      a personal opinion or view of @(Y) is defeated'.\n#pred blawx_initially(is_opinion_of(X,Y))
      :: 'that @(X) is a personal opinion or view of @(Y) holds initially'.\n#pred
      blawx_initially(-is_opinion_of(X,Y)) :: 'that it is not the case that @(X) is
      a personal opinion or view of @(Y) holds initially'.\n#pred blawx_ultimately(is_opinion_of(X,Y))
      :: 'that @(X) is a personal opinion or view of @(Y) holds ultimately'.\n#pred
      blawx_ultimately(-is_opinion_of(X,Y)) :: 'that it is not the case that @(X)
      is a personal opinion or view of @(Y) holds ultimately'.\n#pred blawx_as_of(is_opinion_of(X,Y),T)
      :: 'that @(X) is a personal opinion or view of @(Y) holds at @(T)'.\n#pred blawx_as_of(-is_opinion_of(X,Y),T)
      :: 'that it is not the case that @(X) is a personal opinion or view of @(Y)
      holds at @(T)'.\n#pred blawx_during(T1,is_opinion_of(X,Y),T2) :: 'that @(X)
      is a personal opinion or view of @(Y) held between @(T1) and @(T2)'.\n#pred
      blawx_during(T1,-is_opinion_of(X,Y),T2) :: 'that it is not the case that @(X)
      is a personal opinion or view of @(Y) held between @(T1) and @(T2)'.\n#pred
      blawx_becomes(is_opinion_of(X,Y),T) :: 'that @(X) is a personal opinion or view
      of @(Y) became true at @(T)'.\n#pred blawx_becomes(-is_opinion_of(X,Y),T) ::
      'that it is not the case that @(X) is a personal opinion or view of @(Y) became
      true at @(T)'.\n#pred blawx_not_interrupted(Start,is_opinion_of(X,Y),End) ::
      '@(X) is a personal opinion or view of @(Y) remained the case between @(Start)
      and @(End)'.\n#pred blawx_not_interrupted(Start,-is_opinion_of(X,Y),End) ::
      'it is not the case that @(X) is a personal opinion or view of @(Y) remained
      the case between @(Start) and @(End)'.\nblawx_not_interrupted(datetime(Start),is_opinion_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-is_opinion_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-is_opinion_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(is_opinion_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),is_opinion_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-is_opinion_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-is_opinion_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(is_opinion_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),is_opinion_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-is_opinion_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-is_opinion_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(is_opinion_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),is_opinion_of(X,Y),datetime(eot))
      :- blawx_initially(is_opinion_of(X,Y)), blawx_ultimately(is_opinion_of(X,Y)),
      findall(Time,blawx_becomes(-is_opinion_of(X,Y),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-is_opinion_of(X,Y),datetime(eot))
      :- blawx_initially(-is_opinion_of(X,Y)), blawx_ultimately(-is_opinion_of(X,Y)),
      findall(Time,blawx_becomes(is_opinion_of(X,Y),datetime(Time)),[]).\nblawx_as_of(is_opinion_of(X,Y),datetime(Time))
      :- blawx_initially(is_opinion_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),is_opinion_of(X,Y),datetime(BeforeT)).\nblawx_as_of(is_opinion_of(X,Y),datetime(Time))
      :- blawx_becomes(is_opinion_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),is_opinion_of(X,Y),datetime(Time)).\nblawx_as_of(-is_opinion_of(X,Y),datetime(Time))
      :- blawx_initially(-is_opinion_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),-is_opinion_of(X,Y),datetime(BeforeT)).\nblawx_as_of(-is_opinion_of(X,Y),datetime(Time))
      :- blawx_becomes(-is_opinion_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),-is_opinion_of(X,Y),datetime(Time)).\nblawx_during(datetime(Start),is_opinion_of(X,Y),datetime(End))
      :- blawx_becomes(is_opinion_of(X,Y),datetime(Start)), blawx_becomes(-is_opinion_of(X,Y),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),is_opinion_of(X,Y),datetime(End)).\nblawx_during(datetime(bot),is_opinion_of(X,Y),datetime(End))
      :- blawx_initially(is_opinion_of(X,Y)), blawx_becomes(-is_opinion_of(X,Y),datetime(End)),
      blawx_not_interrupted(datetime(bot),is_opinion_of(X,Y),datetime(End)).\nblawx_during(datetime(Start),is_opinion_of(X,Y),datetime(eot))
      :- blawx_ultimately(is_opinion_of(X,Y)), blawx_becomes(-is_opinion_of(X,Y),datetime(Start)),
      blawx_not_interrupted(datetime(Start),is_opinion_of(X,Y),datetime(eot)).\nblawx_during(datetime(bot),is_opinion_of(X,Y),datetime(eot))
      :- blawx_initially(is_opinion_of(X,Y)), blawx_ultimately(is_opinion_of(X,Y)),
      blawx_becomes(-is_opinion_of(X,Y),datetime(Start)), blawx_not_interrupted(datetime(bot),is_opinion_of(X,Y),datetime(eot)).\nblawx_during(datetime(Start),-is_opinion_of(X,Y),datetime(End))
      :- blawx_becomes(-is_opinion_of(X,Y),datetime(Start)), blawx_becomes(is_opinion_of(X,Y),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),-is_opinion_of(X,Y),datetime(End)).\nblawx_during(datetime(bot),-is_opinion_of(X,Y),datetime(End))
      :- blawx_initially(-is_opinion_of(X,Y)), blawx_becomes(is_opinion_of(X,Y),datetime(End)),
      blawx_not_interrupted(datetime(bot),-is_opinion_of(X,Y),datetime(End)).\nblawx_during(datetime(Start),-is_opinion_of(X,Y),datetime(eot))
      :- blawx_ultimately(-is_opinion_of(X,Y)), blawx_becomes(is_opinion_of(X,Y),datetime(Start)),
      blawx_not_interrupted(datetime(Start),-is_opinion_of(X,Y),datetime(eot)).\nblawx_during(datetime(bot),-is_opinion_of(X,Y),datetime(eot))
      :- blawx_initially(-is_opinion_of(X,Y)), blawx_ultimately(-is_opinion_of(X,Y)),
      blawx_becomes(-is_opinion_of(X,Y),datetime(Start)), blawx_not_interrupted(datetime(bot),-is_opinion_of(X,Y),datetime(eot)).\n\naccording_to(sec_3__subsec_7__para_d_section,relating_to,Info,Person)
      :- blawx_applies(sec_3__subsec_7__para_d_section,Info),\nblawx_applies(sec_3__subsec_7__para_d_section,Person),\ninformation(Info),\nindividual(Person),\nidentifiably_about(Info,Person),\nrecorded(Info),\nis_opinion_of(Info,Person).\n\n%
      BLAWX CHECK DUPLICATES\nholds(sec_3__subsec_7__para_d_section,relating_to,Info,Person)
      :- according_to(sec_3__subsec_7__para_d_section,relating_to,Info,Person).\n\n%
      BLAWX CHECK DUPLICATES\n  relating_to(Info,Person) :- holds(sec_3__subsec_7__para_d_section,relating_to,Info,Person)."
