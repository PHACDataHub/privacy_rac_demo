    scasp_encoding: "blawx_category(purpose).\nblawx_category_nlg(purpose,\"\",\"is
      a purpose\").\n:- dynamic purpose/1.\n#pred purpose(X) :: '@(X) is a purpose'.\n#pred
      holds(user,purpose,X) :: 'it is provided as a fact that @(X) is a purpose'.\n#pred
      holds(user,-purpose,X) :: 'it is provided as a fact that it is not the case
      that @(X) is a purpose'.\n#pred holds(Z,purpose,X) :: 'the conclusion in @(Z)
      that @(X) is a purpose holds'.\n#pred holds(Z,-purpose,X) :: 'the conclusion
      in @(Z) that it is not the case that @(X) is a purpose holds'.\n#pred according_to(Z,purpose,X)
      :: 'according to @(Z), @(X) is a purpose'.\n#pred according_to(Z,-purpose,X)
      :: 'according to @(Z), it is not the case that @(X) is a purpose'.\n#pred blawx_defeated(Z,purpose,X)
      :: 'the conclusion in @(Z) that @(X) is a purpose is defeated'.\n#pred blawx_defeated(Z,-purpose,X)
      :: 'the conclusion in @(Z) that it is not the case that @(X) is a purpose is
      defeated'.\n#pred blawx_initially(purpose(X)) :: 'that @(X) is a purpose holds
      initially'.\n#pred blawx_initially(-purpose(X)) :: 'that it is not the case
      that @(X) is a purpose holds initially'.\n#pred blawx_ultimately(purpose(X))
      :: 'that @(X) is a purpose holds ultimately'.\n#pred blawx_ultimately(-purpose(X))
      :: 'that it is not the case that @(X) is a purpose holds ultimately'.\n#pred
      blawx_as_of(purpose(X),T) :: 'that @(X) is a purpose holds at @(T)'.\n#pred
      blawx_as_of(-purpose(X),T) :: 'that it is not the case that @(X) is a purpose
      holds at @(T)'.\n#pred blawx_during(T1,purpose(X),T2) :: 'that @(X) is a purpose
      held between @(T1) and @(T2)'.\n#pred blawx_during(T1,-purpose(X),T2) :: 'that
      it is not the case that @(X) is a purpose held between @(T1) and @(T2)'.\n#pred
      blawx_becomes(purpose(X),T) :: 'that @(X) is a purpose became true at @(T)'.\n#pred
      blawx_becomes(-purpose(X),T) :: 'that it is not the case that @(X) is a purpose
      became true at @(T)'.\n#pred blawx_not_interrupted(Start,purpose(X),End) ::
      '@(X) is a purpose remained the case between @(Start) and @(End)'.\n#pred blawx_not_interrupted(Start,-purpose(X),End)
      :: 'it is not the case that @(X) is a purpose remained the case between @(Start)
      and @(End)'.\nblawx_not_interrupted(datetime(Start),purpose(X),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-purpose(X),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-purpose(X),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(purpose(X),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),purpose(X),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-purpose(X),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-purpose(X),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(purpose(X),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),purpose(X),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-purpose(X),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-purpose(X),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(purpose(X),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),purpose(X),datetime(eot))
      :- blawx_initially(purpose(X)), blawx_ultimately(purpose(X)), findall(Time,blawx_becomes(-purpose(X),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-purpose(X),datetime(eot))
      :- blawx_initially(-purpose(X)), blawx_ultimately(-purpose(X)), findall(Time,blawx_becomes(purpose(X),datetime(Time)),[]).\nblawx_as_of(purpose(X),datetime(Time))
      :- blawx_initially(purpose(X)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),purpose(X),datetime(BeforeT)).\nblawx_as_of(purpose(X),datetime(Time))
      :- blawx_becomes(purpose(X),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),purpose(X),datetime(Time)).\nblawx_as_of(-purpose(X),datetime(Time))
      :- blawx_initially(-purpose(X)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),-purpose(X),datetime(BeforeT)).\nblawx_as_of(-purpose(X),datetime(Time))
      :- blawx_becomes(-purpose(X),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),-purpose(X),datetime(Time)).\nblawx_during(datetime(Start),purpose(X),datetime(End))
      :- blawx_becomes(purpose(X),datetime(Start)), blawx_becomes(-purpose(X),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),purpose(X),datetime(End)).\nblawx_during(datetime(bot),purpose(X),datetime(End))
      :- blawx_initially(purpose(X)), blawx_becomes(-purpose(X),datetime(End)), blawx_not_interrupted(datetime(bot),purpose(X),datetime(End)).\nblawx_during(datetime(Start),purpose(X),datetime(eot))
      :- blawx_ultimately(purpose(X)), blawx_becomes(-purpose(X),datetime(Start)),
      blawx_not_interrupted(datetime(Start),purpose(X),datetime(eot)).\nblawx_during(datetime(bot),purpose(X),datetime(eot))
      :- blawx_initially(purpose(X)), blawx_ultimately(purpose(X)), blawx_becomes(-purpose(X),datetime(Start)),
      blawx_not_interrupted(datetime(bot),purpose(X),datetime(eot)).\nblawx_during(datetime(Start),-purpose(X),datetime(End))
      :- blawx_becomes(-purpose(X),datetime(Start)), blawx_becomes(purpose(X),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),-purpose(X),datetime(End)).\nblawx_during(datetime(bot),-purpose(X),datetime(End))
      :- blawx_initially(-purpose(X)), blawx_becomes(purpose(X),datetime(End)), blawx_not_interrupted(datetime(bot),-purpose(X),datetime(End)).\nblawx_during(datetime(Start),-purpose(X),datetime(eot))
      :- blawx_ultimately(-purpose(X)), blawx_becomes(purpose(X),datetime(Start)),
      blawx_not_interrupted(datetime(Start),-purpose(X),datetime(eot)).\nblawx_during(datetime(bot),-purpose(X),datetime(eot))
      :- blawx_initially(-purpose(X)), blawx_ultimately(-purpose(X)), blawx_becomes(-purpose(X),datetime(Start)),
      blawx_not_interrupted(datetime(bot),-purpose(X),datetime(eot)).\nblawx_attribute(information,under_control_of,entity).\nblawx_attribute_nlg(under_control_of,ov,\"\",\"is
      under the control of\",\"\").\n:- dynamic under_control_of/2.\n#pred under_control_of(X,Y)
      :: '@(X) is under the control of @(Y)'.\n#pred holds(user,under_control_of,X,Y)
