    scasp_encoding: "blawx_attribute(information,contains,information).\nblawx_attribute_nlg(contains,ov,\"\",\"contains\",\"\").\n:-
      dynamic contains/2.\n#pred contains(X,Y) :: '@(X) contains @(Y)'.\n#pred holds(user,contains,X,Y)
      :: 'it is provided as a fact that @(X) contains @(Y)'.\n#pred holds(user,-contains,X,Y)
      :: 'it is provided as a fact that it is not the case that @(X) contains @(Y)'.\n#pred
      holds(Z,contains,X,Y) :: 'the conclusion in @(Z) that @(X) contains @(Y) holds'.\n#pred
      holds(Z,-contains,X,Y) :: 'the conclusion in @(Z) that it is not the case that
      @(X) contains @(Y) holds'.\n#pred according_to(Z,contains,X,Y) :: 'according
      to @(Z), @(X) contains @(Y)'.\n#pred according_to(Z,-contains,X,Y) :: 'according
      to @(Z), it is not the case that @(X) contains @(Y)'.\n#pred blawx_defeated(Z,contains,X,Y)
      :: 'the conclusion in @(Z) that @(X) contains @(Y) is defeated'.\n#pred blawx_defeated(Z,-contains,X,Y)
      :: 'the conclusion in @(Z) that @(X) contains @(Y) is defeated'.\n#pred blawx_initially(contains(X,Y))
      :: 'that @(X) contains @(Y) holds initially'.\n#pred blawx_initially(-contains(X,Y))
      :: 'that it is not the case that @(X) contains @(Y) holds initially'.\n#pred
      blawx_ultimately(contains(X,Y)) :: 'that @(X) contains @(Y) holds ultimately'.\n#pred
      blawx_ultimately(-contains(X,Y)) :: 'that it is not the case that @(X) contains
      @(Y) holds ultimately'.\n#pred blawx_as_of(contains(X,Y),T) :: 'that @(X) contains
      @(Y) holds at @(T)'.\n#pred blawx_as_of(-contains(X,Y),T) :: 'that it is not
      the case that @(X) contains @(Y) holds at @(T)'.\n#pred blawx_during(T1,contains(X,Y),T2)
      :: 'that @(X) contains @(Y) held between @(T1) and @(T2)'.\n#pred blawx_during(T1,-contains(X,Y),T2)
      :: 'that it is not the case that @(X) contains @(Y) held between @(T1) and @(T2)'.\n#pred
      blawx_becomes(contains(X,Y),T) :: 'that @(X) contains @(Y) became true at @(T)'.\n#pred
      blawx_becomes(-contains(X,Y),T) :: 'that it is not the case that @(X) contains
      @(Y) became true at @(T)'.\n#pred blawx_not_interrupted(Start,contains(X,Y),End)
      :: '@(X) contains @(Y) remained the case between @(Start) and @(End)'.\n#pred
      blawx_not_interrupted(Start,-contains(X,Y),End) :: 'it is not the case that
      @(X) contains @(Y) remained the case between @(Start) and @(End)'.\nblawx_not_interrupted(datetime(Start),contains(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-contains(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-contains(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(contains(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),contains(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-contains(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-contains(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(contains(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),contains(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-contains(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-contains(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(contains(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),contains(X,Y),datetime(eot))
      :- blawx_initially(contains(X,Y)), blawx_ultimately(contains(X,Y)), findall(Time,blawx_becomes(-contains(X,Y),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-contains(X,Y),datetime(eot))
      :- blawx_initially(-contains(X,Y)), blawx_ultimately(-contains(X,Y)), findall(Time,blawx_becomes(contains(X,Y),datetime(Time)),[]).\nblawx_as_of(contains(X,Y),datetime(Time))
      :- blawx_initially(contains(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),contains(X,Y),datetime(BeforeT)).\nblawx_as_of(contains(X,Y),datetime(Time))
      :- blawx_becomes(contains(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),contains(X,Y),datetime(Time)).\nblawx_as_of(-contains(X,Y),datetime(Time))
      :- blawx_initially(-contains(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),-contains(X,Y),datetime(BeforeT)).\nblawx_as_of(-contains(X,Y),datetime(Time))
      :- blawx_becomes(-contains(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),-contains(X,Y),datetime(Time)).\nblawx_during(datetime(Start),contains(X,Y),datetime(End))
      :- blawx_becomes(contains(X,Y),datetime(Start)), blawx_becomes(-contains(X,Y),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),contains(X,Y),datetime(End)).\nblawx_during(datetime(bot),contains(X,Y),datetime(End))
      :- blawx_initially(contains(X,Y)), blawx_becomes(-contains(X,Y),datetime(End)),
      blawx_not_interrupted(datetime(bot),contains(X,Y),datetime(End)).\nblawx_during(datetime(Start),contains(X,Y),datetime(eot))
      :- blawx_ultimately(contains(X,Y)), blawx_becomes(-contains(X,Y),datetime(Start)),
      blawx_not_interrupted(datetime(Start),contains(X,Y),datetime(eot)).\nblawx_during(datetime(bot),contains(X,Y),datetime(eot))
      :- blawx_initially(contains(X,Y)), blawx_ultimately(contains(X,Y)), blawx_becomes(-contains(X,Y),datetime(Start)),
      blawx_not_interrupted(datetime(bot),contains(X,Y),datetime(eot)).\nblawx_during(datetime(Start),-contains(X,Y),datetime(End))
      :- blawx_becomes(-contains(X,Y),datetime(Start)), blawx_becomes(contains(X,Y),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),-contains(X,Y),datetime(End)).\nblawx_during(datetime(bot),-contains(X,Y),datetime(End))
      :- blawx_initially(-contains(X,Y)), blawx_becomes(contains(X,Y),datetime(End)),
      blawx_not_interrupted(datetime(bot),-contains(X,Y),datetime(End)).\nblawx_during(datetime(Start),-contains(X,Y),datetime(eot))
      :- blawx_ultimately(-contains(X,Y)), blawx_becomes(contains(X,Y),datetime(Start)),
      blawx_not_interrupted(datetime(Start),-contains(X,Y),datetime(eot)).\nblawx_during(datetime(bot),-contains(X,Y),datetime(eot))
      :- blawx_initially(-contains(X,Y)), blawx_ultimately(-contains(X,Y)), blawx_becomes(-contains(X,Y),datetime(Start)),
      blawx_not_interrupted(datetime(bot),-contains(X,Y),datetime(eot)).\nblawx_attribute(information,is_name_of,individual).\nblawx_attribute_nlg(is_name_of,ov,\"\",\"is
      the name of\",\"\").\n:- dynamic is_name_of/2.\n#pred is_name_of(X,Y) :: '@(X)
      is the name of @(Y)'.\n#pred holds(user,is_name_of,X,Y) :: 'it is provided as
      a fact that @(X) is the name of @(Y)'.\n#pred holds(user,-is_name_of,X,Y) ::
      'it is provided as a fact that it is not the case that @(X) is the name of @(Y)'.\n#pred
      holds(Z,is_name_of,X,Y) :: 'the conclusion in @(Z) that @(X) is the name of
      @(Y) holds'.\n#pred holds(Z,-is_name_of,X,Y) :: 'the conclusion in @(Z) that
      it is not the case that @(X) is the name of @(Y) holds'.\n#pred according_to(Z,is_name_of,X,Y)
      :: 'according to @(Z), @(X) is the name of @(Y)'.\n#pred according_to(Z,-is_name_of,X,Y)
      :: 'according to @(Z), it is not the case that @(X) is the name of @(Y)'.\n#pred
      blawx_defeated(Z,is_name_of,X,Y) :: 'the conclusion in @(Z) that @(X) is the
      name of @(Y) is defeated'.\n#pred blawx_defeated(Z,-is_name_of,X,Y) :: 'the
      conclusion in @(Z) that @(X) is the name of @(Y) is defeated'.\n#pred blawx_initially(is_name_of(X,Y))
      :: 'that @(X) is the name of @(Y) holds initially'.\n#pred blawx_initially(-is_name_of(X,Y))
      :: 'that it is not the case that @(X) is the name of @(Y) holds initially'.\n#pred
      blawx_ultimately(is_name_of(X,Y)) :: 'that @(X) is the name of @(Y) holds ultimately'.\n#pred
      blawx_ultimately(-is_name_of(X,Y)) :: 'that it is not the case that @(X) is
