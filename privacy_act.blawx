    scasp_encoding: "% It's not clear how \"identifying\" is supposed to be applied\n%
      here. There is already a requirement that the information\n% be identifying.
      Here the word identifying may reiterate that\n% requirement, or it may apply
      more specifically to the number,\n% or it may apply more specifically to numbers,
      symbols, and\n% particulars. I'm taking the later position, that this section\n%
      refers to particulars that are identifying in their own\n% right, not only as
      informations. But I'm not making it a\n% seperate requirement, I'm including
      it in the description.\nblawx_attribute(information,is_an_id_number_of,individual).\nblawx_attribute_nlg(is_an_id_number_of,ov,\"\",\"is
      an identifying number assigned to\",\"\").\n:- dynamic is_an_id_number_of/2.\n#pred
      is_an_id_number_of(X,Y) :: '@(X) is an identifying number assigned to @(Y)'.\n#pred
      holds(user,is_an_id_number_of,X,Y) :: 'it is provided as a fact that @(X) is
      an identifying number assigned to @(Y)'.\n#pred holds(user,-is_an_id_number_of,X,Y)
      :: 'it is provided as a fact that it is not the case that @(X) is an identifying
      number assigned to @(Y)'.\n#pred holds(Z,is_an_id_number_of,X,Y) :: 'the conclusion
      in @(Z) that @(X) is an identifying number assigned to @(Y) holds'.\n#pred holds(Z,-is_an_id_number_of,X,Y)
      :: 'the conclusion in @(Z) that it is not the case that @(X) is an identifying
      number assigned to @(Y) holds'.\n#pred according_to(Z,is_an_id_number_of,X,Y)
      :: 'according to @(Z), @(X) is an identifying number assigned to @(Y)'.\n#pred
      according_to(Z,-is_an_id_number_of,X,Y) :: 'according to @(Z), it is not the
      case that @(X) is an identifying number assigned to @(Y)'.\n#pred blawx_defeated(Z,is_an_id_number_of,X,Y)
      :: 'the conclusion in @(Z) that @(X) is an identifying number assigned to @(Y)
      is defeated'.\n#pred blawx_defeated(Z,-is_an_id_number_of,X,Y) :: 'the conclusion
      in @(Z) that @(X) is an identifying number assigned to @(Y) is defeated'.\n#pred
      blawx_initially(is_an_id_number_of(X,Y)) :: 'that @(X) is an identifying number
      assigned to @(Y) holds initially'.\n#pred blawx_initially(-is_an_id_number_of(X,Y))
      :: 'that it is not the case that @(X) is an identifying number assigned to @(Y)
      holds initially'.\n#pred blawx_ultimately(is_an_id_number_of(X,Y)) :: 'that
      @(X) is an identifying number assigned to @(Y) holds ultimately'.\n#pred blawx_ultimately(-is_an_id_number_of(X,Y))
      :: 'that it is not the case that @(X) is an identifying number assigned to @(Y)
      holds ultimately'.\n#pred blawx_as_of(is_an_id_number_of(X,Y),T) :: 'that @(X)
      is an identifying number assigned to @(Y) holds at @(T)'.\n#pred blawx_as_of(-is_an_id_number_of(X,Y),T)
      :: 'that it is not the case that @(X) is an identifying number assigned to @(Y)
      holds at @(T)'.\n#pred blawx_during(T1,is_an_id_number_of(X,Y),T2) :: 'that
      @(X) is an identifying number assigned to @(Y) held between @(T1) and @(T2)'.\n#pred
      blawx_during(T1,-is_an_id_number_of(X,Y),T2) :: 'that it is not the case that
      @(X) is an identifying number assigned to @(Y) held between @(T1) and @(T2)'.\n#pred
      blawx_becomes(is_an_id_number_of(X,Y),T) :: 'that @(X) is an identifying number
      assigned to @(Y) became true at @(T)'.\n#pred blawx_becomes(-is_an_id_number_of(X,Y),T)
      :: 'that it is not the case that @(X) is an identifying number assigned to @(Y)
      became true at @(T)'.\n#pred blawx_not_interrupted(Start,is_an_id_number_of(X,Y),End)
      :: '@(X) is an identifying number assigned to @(Y) remained the case between
      @(Start) and @(End)'.\n#pred blawx_not_interrupted(Start,-is_an_id_number_of(X,Y),End)
      :: 'it is not the case that @(X) is an identifying number assigned to @(Y) remained
      the case between @(Start) and @(End)'.\nblawx_not_interrupted(datetime(Start),is_an_id_number_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-is_an_id_number_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-is_an_id_number_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(is_an_id_number_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),is_an_id_number_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-is_an_id_number_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-is_an_id_number_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(is_an_id_number_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),is_an_id_number_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-is_an_id_number_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-is_an_id_number_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(is_an_id_number_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),is_an_id_number_of(X,Y),datetime(eot))
      :- blawx_initially(is_an_id_number_of(X,Y)), blawx_ultimately(is_an_id_number_of(X,Y)),
      findall(Time,blawx_becomes(-is_an_id_number_of(X,Y),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-is_an_id_number_of(X,Y),datetime(eot))
      :- blawx_initially(-is_an_id_number_of(X,Y)), blawx_ultimately(-is_an_id_number_of(X,Y)),
      findall(Time,blawx_becomes(is_an_id_number_of(X,Y),datetime(Time)),[]).\nblawx_as_of(is_an_id_number_of(X,Y),datetime(Time))
      :- blawx_initially(is_an_id_number_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),is_an_id_number_of(X,Y),datetime(BeforeT)).\nblawx_as_of(is_an_id_number_of(X,Y),datetime(Time))
      :- blawx_becomes(is_an_id_number_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),is_an_id_number_of(X,Y),datetime(Time)).\nblawx_as_of(-is_an_id_number_of(X,Y),datetime(Time))
      :- blawx_initially(-is_an_id_number_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),-is_an_id_number_of(X,Y),datetime(BeforeT)).\nblawx_as_of(-is_an_id_number_of(X,Y),datetime(Time))
      :- blawx_becomes(-is_an_id_number_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),-is_an_id_number_of(X,Y),datetime(Time)).\nblawx_during(datetime(Start),is_an_id_number_of(X,Y),datetime(End))
      :- blawx_becomes(is_an_id_number_of(X,Y),datetime(Start)), blawx_becomes(-is_an_id_number_of(X,Y),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),is_an_id_number_of(X,Y),datetime(End)).\nblawx_during(datetime(bot),is_an_id_number_of(X,Y),datetime(End))
      :- blawx_initially(is_an_id_number_of(X,Y)), blawx_becomes(-is_an_id_number_of(X,Y),datetime(End)),
      blawx_not_interrupted(datetime(bot),is_an_id_number_of(X,Y),datetime(End)).\nblawx_during(datetime(Start),is_an_id_number_of(X,Y),datetime(eot))
      :- blawx_ultimately(is_an_id_number_of(X,Y)), blawx_becomes(-is_an_id_number_of(X,Y),datetime(Start)),
      blawx_not_interrupted(datetime(Start),is_an_id_number_of(X,Y),datetime(eot)).\nblawx_during(datetime(bot),is_an_id_number_of(X,Y),datetime(eot))
      :- blawx_initially(is_an_id_number_of(X,Y)), blawx_ultimately(is_an_id_number_of(X,Y)),
