    scasp_encoding: "blawx_attribute(information,relates_to_race_of,individual).\nblawx_attribute_nlg(relates_to_race_of,ov,\"\",\"relates
      to the race of\",\"\").\n:- dynamic relates_to_race_of/2.\n#pred relates_to_race_of(X,Y)
      :: '@(X) relates to the race of @(Y)'.\n#pred holds(user,relates_to_race_of,X,Y)
      :: 'it is provided as a fact that @(X) relates to the race of @(Y)'.\n#pred
      holds(user,-relates_to_race_of,X,Y) :: 'it is provided as a fact that it is
      not the case that @(X) relates to the race of @(Y)'.\n#pred holds(Z,relates_to_race_of,X,Y)
      :: 'the conclusion in @(Z) that @(X) relates to the race of @(Y) holds'.\n#pred
      holds(Z,-relates_to_race_of,X,Y) :: 'the conclusion in @(Z) that it is not the
      case that @(X) relates to the race of @(Y) holds'.\n#pred according_to(Z,relates_to_race_of,X,Y)
      :: 'according to @(Z), @(X) relates to the race of @(Y)'.\n#pred according_to(Z,-relates_to_race_of,X,Y)
      :: 'according to @(Z), it is not the case that @(X) relates to the race of @(Y)'.\n#pred
      blawx_defeated(Z,relates_to_race_of,X,Y) :: 'the conclusion in @(Z) that @(X)
      relates to the race of @(Y) is defeated'.\n#pred blawx_defeated(Z,-relates_to_race_of,X,Y)
      :: 'the conclusion in @(Z) that @(X) relates to the race of @(Y) is defeated'.\n#pred
      blawx_initially(relates_to_race_of(X,Y)) :: 'that @(X) relates to the race of
      @(Y) holds initially'.\n#pred blawx_initially(-relates_to_race_of(X,Y)) :: 'that
      it is not the case that @(X) relates to the race of @(Y) holds initially'.\n#pred
      blawx_ultimately(relates_to_race_of(X,Y)) :: 'that @(X) relates to the race
      of @(Y) holds ultimately'.\n#pred blawx_ultimately(-relates_to_race_of(X,Y))
      :: 'that it is not the case that @(X) relates to the race of @(Y) holds ultimately'.\n#pred
      blawx_as_of(relates_to_race_of(X,Y),T) :: 'that @(X) relates to the race of
      @(Y) holds at @(T)'.\n#pred blawx_as_of(-relates_to_race_of(X,Y),T) :: 'that
      it is not the case that @(X) relates to the race of @(Y) holds at @(T)'.\n#pred
      blawx_during(T1,relates_to_race_of(X,Y),T2) :: 'that @(X) relates to the race
      of @(Y) held between @(T1) and @(T2)'.\n#pred blawx_during(T1,-relates_to_race_of(X,Y),T2)
      :: 'that it is not the case that @(X) relates to the race of @(Y) held between
      @(T1) and @(T2)'.\n#pred blawx_becomes(relates_to_race_of(X,Y),T) :: 'that @(X)
      relates to the race of @(Y) became true at @(T)'.\n#pred blawx_becomes(-relates_to_race_of(X,Y),T)
      :: 'that it is not the case that @(X) relates to the race of @(Y) became true
      at @(T)'.\n#pred blawx_not_interrupted(Start,relates_to_race_of(X,Y),End) ::
      '@(X) relates to the race of @(Y) remained the case between @(Start) and @(End)'.\n#pred
      blawx_not_interrupted(Start,-relates_to_race_of(X,Y),End) :: 'it is not the
      case that @(X) relates to the race of @(Y) remained the case between @(Start)
      and @(End)'.\nblawx_not_interrupted(datetime(Start),relates_to_race_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(-relates_to_race_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(Start),-relates_to_race_of(X,Y),datetime(End))
      :- Start \\= bot, End \\= eot, findall(Time,blawx_becomes(relates_to_race_of(X,Y),datetime(Time)),Times),blawx_list_not_between(Times,Start,End).\nblawx_not_interrupted(datetime(bot),relates_to_race_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(-relates_to_race_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(bot),-relates_to_race_of(X,Y),datetime(End))
      :- End \\= eot, findall(Time,blawx_becomes(relates_to_race_of(X,Y),datetime(Time)),Times),blawx_list_not_before(Times,End).\nblawx_not_interrupted(datetime(Start),relates_to_race_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(-relates_to_race_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(Start),-relates_to_race_of(X,Y),datetime(eot))
      :- Start \\= bot, findall(Time,blawx_becomes(relates_to_race_of(X,Y),datetime(Time)),Times),blawx_list_not_after(Times,Start).\nblawx_not_interrupted(datetime(bot),relates_to_race_of(X,Y),datetime(eot))
      :- blawx_initially(relates_to_race_of(X,Y)), blawx_ultimately(relates_to_race_of(X,Y)),
      findall(Time,blawx_becomes(-relates_to_race_of(X,Y),datetime(Time)),[]).\nblawx_not_interrupted(datetime(bot),-relates_to_race_of(X,Y),datetime(eot))
      :- blawx_initially(-relates_to_race_of(X,Y)), blawx_ultimately(-relates_to_race_of(X,Y)),
      findall(Time,blawx_becomes(relates_to_race_of(X,Y),datetime(Time)),[]).\nblawx_as_of(relates_to_race_of(X,Y),datetime(Time))
      :- blawx_initially(relates_to_race_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),relates_to_race_of(X,Y),datetime(BeforeT)).\nblawx_as_of(relates_to_race_of(X,Y),datetime(Time))
      :- blawx_becomes(relates_to_race_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),relates_to_race_of(X,Y),datetime(Time)).\nblawx_as_of(-relates_to_race_of(X,Y),datetime(Time))
      :- blawx_initially(-relates_to_race_of(X,Y)), BeforeT #< Time,blawx_not_interrupted(datetime(bot),-relates_to_race_of(X,Y),datetime(BeforeT)).\nblawx_as_of(-relates_to_race_of(X,Y),datetime(Time))
      :- blawx_becomes(-relates_to_race_of(X,Y),datetime(BeforeT)),BeforeT #< Time,blawx_not_interrupted(datetime(BeforeT),-relates_to_race_of(X,Y),datetime(Time)).\nblawx_during(datetime(Start),relates_to_race_of(X,Y),datetime(End))
      :- blawx_becomes(relates_to_race_of(X,Y),datetime(Start)), blawx_becomes(-relates_to_race_of(X,Y),datetime(End)),
      Start #< End, blawx_not_interrupted(datetime(Start),relates_to_race_of(X,Y),datetime(End)).\nblawx_during(datetime(bot),relates_to_race_of(X,Y),datetime(End))
      :- blawx_initially(relates_to_race_of(X,Y)), blawx_becomes(-relates_to_race_of(X,Y),datetime(End)),
      blawx_not_interrupted(datetime(bot),relates_to_race_of(X,Y),datetime(End)).\nblawx_during(datetime(Start),relates_to_race_of(X,Y),datetime(eot))
      :- blawx_ultimately(relates_to_race_of(X,Y)), blawx_becomes(-relates_to_race_of(X,Y),datetime(Start)),
      blawx_not_interrupted(datetime(Start),relates_to_race_of(X,Y),datetime(eot)).\nblawx_during(datetime(bot),relates_to_race_of(X,Y),datetime(eot))
